{
  "name": "modern-async",
  "version": "0.1.0",
  "description": "A modern tooling library for asynchronous operations using async/await and promises",
  "main": "dist/modern-async.cjs",
  "module": "src/modern-async.mjs",
  "scripts": {
    "build": "rollup -c rollup.config.js",
    "test": "npm run lint && npm run coverage",
    "coverage": "npm run jest-node -- --coverage",
    "lint": "eslint \"src/**/*.mjs\"",
    "jest-node": "jest --config=./jest-node.config.js",
    "docs": "jsdoc -c .jsdoc.json && node dev/docs-collect-version-numbers.js",
    "babel": "babel --ignore \"**/*.test.js\" --out-dir dist/babel --config-file ./babel.config.js src"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nicolas-van/modern-async.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/nicolas-van/modern-async/issues"
  },
  "homepage": "https://github.com/nicolas-van/modern-async#readme",
  "devDependencies": {
    "@babel/cli": "^7.12.10",
    "@babel/core": "^7.12.10",
    "@babel/preset-env": "^7.12.10",
    "babel-jest": "^26.6.3",
    "clean-jsdoc-theme": "^3.0.0",
    "eslint": "^6.6.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jest": "^23.0.2",
    "eslint-plugin-jsdoc": "^17.1.0",
    "eslint-plugin-node": "^10.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "jest": "^26.6.3",
    "jsdoc": "^3.6.6",
    "lodash": "^4.17.20",
    "rollup": "^2.33.1",
    "semver-sort": "0.0.4"
  },
  "dependencies": {
    "assert": "^2.0.0"
  }
}
